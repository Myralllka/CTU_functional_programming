(check-equal? (img->mat test_img) '((255.0 255.0 174.28 180.07 255.0) (255.0 174.69 174.28 174.1 255.0) (171.46 174.28 174.28 174.28 172.69) (174.28 174.28 174.28 174.28 173.69)) "You made an error!")

(define test_mat (img->mat test_img))

(define (shrink_to_fit mat block_height block_width)
    (define width (- (length (car mat)) (remainder (length (car mat)) block_width)))
    (define height (- (length mat) (remainder (length mat) block_height)))
    (define (remove_last_n lst n [counter 0]) (
        cond ([= counter n] lst)
        (else (remove_last_n (reverse (cdr (reverse lst))) n (+ counter 1)))
    ))
    (define res_mat (remove_last_n mat (- (length mat) height)))
    (
        map (lambda (x) (remove_last_n x (- (length (car res_mat)) width))) res_mat
    )
)

(check-equal? (shrink_to_fit test_mat 1 1) '((255.0 255.0 174.28 180.07 255.0) (255.0 174.69 174.28 174.1 255.0) (171.46 174.28 174.28 174.28 172.69) (174.28 174.28 174.28 174.28 173.69)) "You made an error!")
(check-equal? (shrink_to_fit test_mat 3 2) '((255.0 255.0 174.28 180.07) (255.0 174.69 174.28 174.1) (171.46 174.28 174.28 174.28)) "You made an error!")
(check-equal? (shrink_to_fit test_mat 3 3) '((255.0 255.0 174.28) (255.0 174.69 174.28) (171.46 174.28 174.28)) "You made an error!")

(define chars " .,:;ox%#@")

(define (generalize mat block_height block_width [result '()])
    (define tmp (list (map (lambda (x) (/ x (* block_width block_height))) (map (lambda (x) (foldl + 0 x)) ((split_on block_width) (apply map + (take mat block_height)))))))
    (cond 
        ([= (length mat) block_height] (append result tmp))
        (else (generalize (drop mat block_height) block_height block_width (append result tmp)))
))

(define (mat_to_list mat c)
    (define chrs (string->list c))
    (define k (lambda (number) (list-ref chrs (inexact->exact (floor (/ (* (length chrs) (- 255 (floor number))) 256))))))
    (map (lambda (x) (map k x)) mat)
)

(define (list_to_str mat)
    (string-append (string-join (map list->string mat) "\n") "\n")
    ; (string-join (map list->string mat) "\n")
)

(define (ascii-art width height chars)
    (lambda (image) (list_to_str (mat_to_list (generalize (shrink_to_fit (img->mat image) height width) height width) chars)))
)

