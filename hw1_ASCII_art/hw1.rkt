(provide img->mat ascii-art)

(define (RGB->grayscale color)
    (+ (* 0.3 (color-red color))
    (* 0.59 (color-green color))
    (* 0.11 (color-blue color)))
)
(define img_arnold (bitmap "arnold.jpg"))

(define (split_on width)
    (lambda (lst [accumulator '()] [result '()]) 
        (   
            cond ([null? lst] (append result (list accumulator)))
                ([= (length accumulator) width] ((split_on width) lst '() (append result (list accumulator))))
                (else  ((split_on width) (cdr lst) (append accumulator (list (car lst))) result))
        )
    )
)

(define (img->mat img) 
    (define width (image-width img))
    (define one_dim (map RGB->grayscale (image->color-list img)))
    ((split_on width) one_dim)
)

